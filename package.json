  "scripts": {
    "env": "chmod +x ./config/env.sh && ./config/env.sh && mv graphkb-env-config.js ./src/static",
    "clean": "rm -r dist stats.json",
    "build:docker": "NODE_ENV=production webpack --mode production --config config/webpack/docker.js",
    "build:dev": "NODE_ENV=development webpack --mode development --config config/webpack/dev.js",
    "build:staging": "NODE_ENV=production webpack --mode production --config config/webpack/staging.js",
    "build:ci": "NODE_ENV=production webpack --mode production --config config/webpack/ci.js",
    "compress": "tar cvfz v${npm_package_version}.tar.gz dist",
    "analyze": "NODE_ENV=production webpack --profile --json --config config/webpack/prod.js > stats.json; webpack-bundle-analyzer stats.json dist/production",
    "start": "npm run env; webpack serve --config config/webpack/dev.js",
    "start:local": "npm run env; webpack serve --config config/webpack/local.js",
    "test:local": "NODE_ENV=local jest --config config/jest.config.js",
    "test": "NODE_ENV=test jest --config config/jest.config.js",
    "test:watch": "NODE_ENV=test jest --config config/jest.config.js --watch",
    "test:ci": "NODE_ENV=test jest --config config/jest.config.js",
    "lint": "eslint src --config .eslintrc.json --quiet",
    "serve": "NODE_ENV=development serve dist/development -s",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
