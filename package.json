{
  "name": "knowledgebase-web",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@bcgsc/knowledgebase-parser": "^0.5.2",
    "@bcgsc/knowledgebase-schema": "^1.3.5",
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "d3-drag": "^1.2.3",
    "d3-force": "^1.1.2",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-zoom": "^1.7.3",
    "downshift": "^2.2.0",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "install": "^0.12.2",
    "json-cycle": "^1.3.0",
    "jsonwebtoken": "^8.3.0",
    "keycloak-js": "^4.7.0",
    "lodash.debounce": "^4.0.8",
    "lodash.omit": "^4.5.0",
    "marked": "^0.5.2",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "raf": "^3.4.0",
    "react": "^16.6.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.4.0-alpha.0",
    "resolve": "1.6.0",
    "serve": "^10.1.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "clean": "rm -r public/{static,bundle.js,main.css} stats.json",
    "build-dev": "webpack -d --mode development",
    "build-prod": "webpack -p --mode production",
    "analyze": "webpack --profile --json > stats.json",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.2",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "react-dev-utils": "^5.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.13"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/components/**/!(index).{js,jsx,mjs}",
      "src/services/**/!(api).{js,jsx,mjs}",
      "src/models/**/*.{js,jsx,mjs}"
    ],
    "coverageReporters": [
      "clover",
      "text",
      "json",
      "json-summary",
      "lcov"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "./coverage/junit.xml"
        }
      ]
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/browserMock.js",
      "<rootDir>/config/jest/enzymeInit.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
