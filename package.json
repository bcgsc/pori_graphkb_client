{
  "name": "knowledgebase-web",
  "version": "0.1.5",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^1.1.0",
    "cypress": "^3.0.2",
    "d3": "^5.5.0",
    "downshift": "^2.1.0",
    "history": "^4.7.2",
    "jsdoc": "^3.5.5",
    "json-cycle": "^1.3.0",
    "jsonwebtoken": "^8.3.0",
    "knowledgebase-parser": "git+ssh://git@svn.bcgsc.ca:7999/vdb/knowledgebase_parser.git#v0.2.0",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.4.0-alpha.0",
    "react-router-native": "^4.3.0-rc.3",
    "react-scripts": "1.1.4",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "jsdoc": "jsdoc -r src -R README.md -d jsdoc --package package.json"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  }
}
