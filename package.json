  "scripts": {
    "clean": "rm -r dist stats.json",
    "build": "NODE_ENV=production webpack -p --config config/webpack/prod.js",
    "build:dev": "NODE_ENV=production webpack -p --mode production --config config/webpack/dev.js",
    "build:staging": "NODE_ENV=production webpack -p --mode production --config config/webpack/staging.js",
    "build:ci": "NODE_ENV=production webpack -p --mode production --config config/webpack/ci.js",
    "compress": "tar cvfz v${npm_package_version}.tar.gz dist",
    "analyze": "NODE_ENV=production webpack --profile --json --config config/webpack/prod.js > stats.json; webpack-bundle-analyzer stats.json dist",
    "start": "webpack-dev-server --config config/webpack/dev.js",
    "start:local": "webpack-dev-server --config config/webpack/local.js",
    "test:local": "NODE_ENV=local jest --config config/jest.config.js",
    "test": "NODE_ENV=test jest --config config/jest.config.js",
    "test:watch": "NODE_ENV=test jest --config config/jest.config.js --watch",
    "test:ci": "NODE_ENV=test jest --config config/jest.config.js",
    "serve": "NODE_ENV=production serve dist -s",
    "commit": "git-cz"
  },
  "sasslintConfig": "config/sass-lint.yml",
  "config": {
    "commitizen": {
